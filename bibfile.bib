@article{__2008-1,
	title = {建筑工程高处坠落事故的故障树分析方法研究},
	issn = {1671-0959},
	url = {https://kns.cnki.net/kns/detail/detail.aspx?QueryID=0&CurRec=1&recid=&FileName=MKSJ200802042&DbName=CJFD2008&DbCode=CJFQ&yx=&pr=&URLID=&bsm=QK0201;},
	abstract = {高处坠落事故是建筑业常见的安全事故之一,它的发生率最高、危险性最大,被业界人士称为"五害之首"。利用系统工程的重要分析方法——故障树分析方法,确定了基本事件的结构重要度,并计算了基本事件的关键重要度和顶事件的发生概率。根据结构重要度的大小,可以编制事故预防方案。根据关键重要度的大小,可以有效地降低顶事件发生的概率。结论对进一步分析高空坠落事故的发生机理有十分重要的指导意义,对预防高空坠落事故的发生有十分重大的现实意义。},
	pages = {112--114},
	number = {2},
	journaltitle = {煤炭工程},
	author = {张, 明轩 and 朱, 月娇 and 翟, 玉杰 and 朱, 晓云 and 高, 全臣},
	urldate = {2020-03-31},
	date = {2008},
	keywords = {安全评价, 高处坠落, 故障树分析, 建筑工程}
}

