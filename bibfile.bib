@thesis{__2008,
	title = {建筑火灾风险评估方法应用研究},
	url = {https://kns.cnki.net/KCMS/detail/detail.aspx?dbcode=CMFD&dbname=CMFD2012&filename=1011053093.nh&v=Mjk0MzdXTTFGckNVUjdxZlpPVnVGeXZrVkxyTlZGMjZIN085SGRIRnJKRWJQSVI4ZVgxTHV4WVM3RGgxVDNxVHI=},
	abstract = {建筑是人类居住、人口高度密集的场所,也是人类财富聚集的地方,特别是随着我国经济高速发展,建筑已成为人、财、物的高密区,一旦发生火灾往往会造成人员伤亡和财物严重损失的灾难性后果,而且建筑火灾是最常见的火灾之一,随着人们生活水平的提高和安全意识的增强,人们对居住的建筑防火安全性提出了新的要求。建筑火灾风险评估可以客观、准确地评价火灾的危险程度,为人们预防火灾、控制火灾以及扑灭火灾提供依据和支持,也是消防设施和消防力量分布、城市规划、科学消防决策的依据,为合理配置消防资源和防火措施,提出科学、经济可行的策略,有效地减少火灾的发生并提高防火安全性。 	论文提出从建筑的某一房间、某一楼层、某一单体建...},
	institution = {昆明理工大学},
	type = {硕士},
	author = {方, 甫兵},
	urldate = {2020-03-31},
	date = {2008},
	keywords = {火灾风险评估, 城中村, 古斯塔夫危险度法, 火灾风险评估工程法, 火灾风险指数法, fire risk assessment, Fire Risk Assessment Method for Engineering, Fire Risk Index Method, Gustav Purt fire assessment, village in city}
}

